    "import numpy as np\n",
    "def sum_primes(n): \n",
    "    ps = np.ones((n >> 1,), dtype=bool) \n",
    "    for i in range(1, int(n**.5 + 1) >> 1): \n",
    "        j = 2 * i + 1 \n",
    "        if ps[i]: ps[(j * j) >> 1::j] = False \n",
    "    return np.sum(np.nonzero(ps)[0] * 2 + 1, dtype = 'i8') + 1 \n",
    "sum_primes(2000000)"
   ]
